{
	"name": "corrupted-svelte",
	"description": "A super lightweight and performant component library for svelte for building corrupted looking websites.",
	"version": "1.1.0",
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package",
		"prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/kit": "^1.27.3",
		"@sveltejs/package": "^1.0.2",
		"svelte": "^3.59.2",
		"svelte-check": "^3.5.2",
		"tslib": "^2.6.2",
		"typescript": "^4.9.5",
		"vite": "^4.5.0"
	},
	"keywords": [
		"svelte",
		"sveltekit",
		"component",
		"corrupted"
	],
	"author": "Jack van der Bilt",
	"license": "MIT",
	"homepage": "https://github.com/UltraCakeBakery/corrupted-text-svelte",
	"private": false,
	"type": "module"
}
